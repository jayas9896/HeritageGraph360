{
  "collections": {
    "event_payloads": {
      "description": "Dynamic event payloads from ingestion streams",
      "example": {
        "tenantId": "org-us-001",
        "eventType": "PROFILE_CREATED",
        "payload": {
          "profileId": "uuid",
          "primaryEmail": "user@example.com",
          "aliases": ["Alias One", "Alias Two"]
        },
        "ingestedAt": "2026-01-15T12:00:00Z"
      }
    },
    "evidence_records": {
      "description": "Scanned documents and evidence metadata",
      "example": {
        "tenantId": "org-us-001",
        "profileId": "uuid",
        "evidenceId": "uuid",
        "sourceType": "ARCHIVE",
        "schemaVersion": "1.0",
        "contentHash": "sha256...",
        "metadata": {
          "archiveId": "A-123",
          "collection": "Census"
        }
      }
    },
    "insight_summaries": {
      "description": "Insight summaries produced by anomaly detection",
      "example": {
        "tenantId": "org-us-001",
        "profileId": "uuid",
        "status": "COMPLETED",
        "summary": "No conflicts detected",
        "createdAt": "2026-01-15T12:00:00Z"
      }
    },
    "sensitive_fields": {
      "description": "Encrypted sensitive profile fields",
      "example": {
        "tenantId": "org-us-001",
        "profileId": "uuid",
        "fieldType": "MEDICAL_HISTORY",
        "encryptedValue": "base64...",
        "createdAt": "2026-01-15T12:00:00Z"
      }
    }
  }
}
